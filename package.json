{
  "name": "blog-website",
  "private": true,
  "version": "0.0.1",
  "workspaces": [
    "apps/*",
    "packages/*",
    "functions/*"
  ],
  "devDependencies": {
    "@types/node": "^24.5.2",
    "eslint": "latest",
    "prettier": "latest",
    "ts-node": "^10.9.2",
    "turbo": "latest",
    "typescript": "latest"
  },
  "scripts": {
    "build": "turbo build",
    "dev": "turbo run dev --parallel",
    "lint": "turbo run lint",
    "test": "turbo run test",
    "generate-launch-json": "ts-node --project tsconfig.scripts.json scripts/generateLaunchJson.ts",
    "prebuild": "npm run generate-launch-json"
  },
  "packageManager": "npm@10.9.2",
  "dependencies": {
    "dotenv": "^17.2.2",
    "otplib": "^12.0.1"
  }
}
